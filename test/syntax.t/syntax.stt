(* Every bit of syntax should appear in this file. *)

check Type

eval Type

def A := Type

axiom B : A

(* Functions *)

check fun (A : Type) => A

check fun (A B C : Type) => A

check fun (A B C : Type) (x y : B) => x

check λ (A : Type) ⇒ A

check λ (A B C : Type) ⇒ A

check (λ x y z ⇒ y) : B → B → B → B

check λ (A B C : Type) (x y : B) ⇒ x
